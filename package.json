{
    "name": "appizza-api",
    "version": "1.0.0",
    "main": "src/server.ts",
    "license": "MIT",
    "scripts": {
        "build": "tsc -w",
        "start": "ts-node-dev --transpileOnly --ignore-watch node_modules src/server.ts",
        "database": "npm run knex:migrate && npm run knex:seed",
        "knex:seed": "knex --knexfile knexfile.ts seed:run",
        "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
        "copy-assets": "copy uploads dist/uploads"
    },
    "dependencies": {
        "celebrate": "^12.1.1",
        "cors": "^2.8.5",
        "cpf-cnpj-validator": "^1.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-jwt": "^5.3.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.1",
        "multer": "^1.4.2",
        "sqlite3": "^4.2.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-jwt": "^0.0.42",
        "@types/hapi__joi": "^17.1.3",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/multer": "^1.4.3",
        "ts-node-dev": "^1.0.0-pre.49",
        "typescript": "^3.9.5"
    }
}
